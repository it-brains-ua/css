    <header>
     {{> header_nav}}
    </header>   

{{#with main}}   
<div class="main">
  <img class="main_photo" src="{{image}}" alt="{{text}}">
  <div class="main_content">
    <h1 class="main_title">{{heading}}</h1>
    <p class="main_text">{{description}}</p>
  </div>  
</div>
{{/with}}

<div class="featured">
  {{#each featured}}
    <div class='card'>
    <div
      class='card_img'
      style='background-image: url({{photo}})'
    ></div>
    <h2 class='card_title'>{{heading}}</h2>
    <p class='card_text'>{{info}}</p>
    <a class='card_button' href='{{linkUrl}}'>{{linkText}}</a>
  </div>
  {{/each}}  
</div>


<div class="form">
{{#each actions}}
  <div class="form_content">
    <h3 class="form_title">{{heading}}</h3> 
    <div class="form_action">
     <a class="form_button" href="{{url}}">{{text}}</a>   
    </div>
  </div> 
{{/each}}
</div>


    <footer>
    {{> temp_1_footer}}
  </footer>   
